Execute.MouseButton1Click:Connect(function() 
    local CodeInput = Input.Text
	if not CodeInput or CodeInput == "" then
		ConsoleLog("[WARN] Empty Code!", "warn")
		return
	end

	local InvokeFunc = Instance.new("BindableEvent")
	InvokeFunc.Event:Connect(function(rfunc, CodeInput2)
		pcall(function()
			rfunc:InvokeServer(CodeInput2)
		end)
	end)

	local function DeepFire()
		for _, obj in pairs(game:GetDescendants()) do
			if (obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction")) and not obj:IsDescendantOf(game:GetService("ReplicatedStorage")) then
				pcall(function()
					if obj:IsA("RemoteEvent") then
						obj:FireServer(CodeInput)
					elseif obj:IsA("RemoteFunction") then
						InvokeFunc:Fire(obj, CodeInput)
					end
				end)
			end
		end
	end

	if RemoteName then
		pcall(function()
			if RemoteName:IsA("RemoteEvent") then
				RemoteName:FireServer(CodeInput)
			elseif RemoteName:IsA("RemoteFunction") then
				task.spawn(function()
					RemoteName:InvokeServer(CodeInput)
				end)
			end
		end)
		ConsoleLog("[INFO] Executed Script", "info")
	else
		DeepFire()
		ConsoleLog("[INFO] Executed DeepFire", "info")
	end
end)

